{
  "main": {
    "key": "main",
    "inner": [
      {
        "tag": "div",
        "class": "sidemenu"
      },
      {
        "tag": "div",
        "class": "overlay",
        "onclick": "%click_overlay%"
      },
      {
        "tag": "div",
        "class": "view",
        "inner": [
          {
            "tag": "div",
            "class": "header",
            "inner": [
              {
                "tag": "div",
                "class": "icon fa fa-%icon% fa-fw fa-lg",
                "onclick": "%click_icon%"
              },
              {
                "tag": "div",
                "class": "title",
                "inner": "%title%",
                "onclick": "%click_title%"
              },
              {
                "tag": "div",
                "class": "user"
              }
            ]
          },
          {
            "tag": "div",
            "class": "content"
          }
        ]
      }
    ]
  },

  "entry": {
    "key": "entry",
    "tag": "div",
    "class": "entry%classes%",
    "title": "%tooltip%",
    "onclick": "%click%",
    "inner": [
      {
        "tag": "div",
        "class": "left",
        "inner": {
          "tag": "div",
          "class": "icon fa fa-%icon% fa-3x fa-fw"
        }
      },
      {
        "tag": "div",
        "class": "right",
        "inner": [
          {
            "tag": "div",
            "class": "title",
            "inner": "%title%"
          },
          {
            "tag": "div",
            "class": "developer",
            "inner": "%developer%"
          },
          {
            "tag": "div",
            "class": "rating"
          }
        ]
      }
    ]
  },

  "component": {
    "key": "component",
    "inner": [
      {
        "tag": "div",
        "class": "header",
        "inner": [
          {
            "tag": "div",
            "class": "left",
            "inner": {
              "tag": "div",
              "class": "icon fa fa-%icon% fa-3x fa-fw"
            }
          },
          {
            "tag": "div",
            "class": "right",
            "inner": [
              {
                "tag": "div",
                "class": "top",
                "inner": [
                  {
                    "tag": "div",
                    "class": "title",
                    "inner": "%title%"
                  },
                  {
                    "tag": "div",
                    "class": "rating"
                  },
                  {
                    "tag": "div",
                    "class": "edit fa fa-%edit%",
                    "title": "edit your publication",
                    "onclick": "%click%"
                  }
                ]
              },
              {
                "tag": "div",
                "class": "abstract",
                "inner": "%abstract%"
              }
            ]
          }
        ]
      },
      {
        "tag": "div",
        "class": "tabmenu"
      }
    ]
  },

  "section": {
    "key": "section",
    "tag": "div",
    "class": "section",
    "inner": [
      {
        "tag": "div",
        "class": "title",
        "inner": "%title%"
      },
      {
        "tag": "div",
        "class": "content"
      }
    ]
  },

  "info": {
    "key": "info",
    "tag": "div",
    "class": "info",
    "inner": [
      {
        "tag": "div",
        "inner": [
          {
            "tag": "div",
            "inner": "Name"
          },
          {
            "tag": "div",
            "inner": "%name%"
          }
        ]
      },
      {
        "tag": "div",
        "inner": [
          {
            "tag": "div",
            "inner": "URL"
          },
          {
            "tag": "div",
            "inner": {
              "tag": "a",
              "target": "_blank",
              "href": "%url%",
              "inner": "%url%"
            }
          }
        ]
      },
      {
        "tag": "div",
        "class": "api",
        "inner": [
          {
            "tag": "div",
            "inner": "API"
          },
          {
            "tag": "div",
            "inner": {
              "tag": "a",
              "target": "_blank",
              "href": "%api%",
              "inner": "%api%"
            }
          }
        ]
      },
      {
        "tag": "div",
        "inner": [
          {
            "tag": "div",
            "inner": "Developer"
          },
          {
            "tag": "div",
            "inner": "%developer%"
          }
        ]
      },
      {
        "tag": "div",
        "inner": [
          {
            "tag": "div",
            "inner": "Licence"
          },
          {
            "tag": "div",
            "inner": "%licence%"
          }
        ]
      }
    ]
  },

  "config": {
    "key": "config",
    "tag": "div",
    "class": "config",
    "inner": [
      {
        "tag": "div",
        "class": "select",
        "inner": [
          {
            "tag": "span",
            "inner": "Switch Factory:"
          },
          {
            "tag": "select",
            "onchange": "%change%"
          }
        ]
      },
      {
        "tag": "div",
        "class": "factory"
      },
      {
        "tag": "div",
        "class": "preview"
      }
    ]
  }
}